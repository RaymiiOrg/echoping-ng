PLUGIN=random.so

include ../Makefile.plugins

ifeq (@RANDOM_BUILD@,1)
all: ${PLUGIN}
else
all:
	@echo "Not building the random plugin"
endif

ifeq (@RANDOM_BUILD@,1)
install: all prepare-install
	${INSTALL} ${PLUGIN} ${DESTDIR}
else
install:
	@echo "Not installing the random plugin"
endif

